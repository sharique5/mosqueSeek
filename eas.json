{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "MAP_API_KEY": "@env.MAP_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "MAP_API_KEY": "@env.MAP_API_KEY_PRODUCTION"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
